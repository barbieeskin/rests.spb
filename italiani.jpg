	<script>
      const onIntersection = entries => {
        for (const entry of entries) {
          if (entry.isIntersecting) {
			var target = entry.target;
			if (target.dataset.style) {
				target.style = target.dataset.style;
			}
			if (target.dataset.src) {
				target.src = target.dataset.src;
			}
			if (target.dataset.srcset) {
				target.srcset = target.dataset.srcset;
			}
            observer.unobserve(entry.target);
          }
        }
      };


    var targetElements = document.querySelectorAll(".iyp_img_deferred");

		if (!window.IntersectionObserver) {

			var extra_offset = window.innerHeight || document.documentElement.clientHeight;

			var isInViewport = function (elem) {
					var bounding = elem.getBoundingClientRect();
					return (
							bounding.top >= 0 &&
							bounding.bottom <= (window.innerHeight || document.documentElement.clientHeight) + elem.scrollHeight + extra_offset
					);
			};

			var throttle = function (func, limit) {
				let lastFunc;
				let lastRan;
				return function() {
					const context = this;
					const args = arguments;
					if (!lastRan) {
						func.apply(context, args);
						lastRan = Date.now();
					} else {
						clearTimeout(lastFunc);
						lastFunc = setTimeout(function() {
							if (Date.now() - lastRan >= limit) {
								func.apply(context, args);
								lastRan = Date.now();
							}
						}, limit - (Date.now() - lastRan));
					}
				};
			}

			var imgDeferredScroll = function () {

				for (var i=0 ; i< targetElements.length ; i++) {
					var el = targetElements[i];

					if (!el.classList.contains('iyp_img_deferred') || !isInViewport(el)) {
							continue;
					}

					if (el.dataset.style) {
						el.style = el.dataset.style;
						el.classList.remove('iyp_img_deferred');
					}
					if (el.dataset.src) {
						el.src = el.dataset.src;
						el.classList.remove('iyp_img_deferred');
					}
					if (el.dataset.srcset) {
						el.srcset = el.dataset.srcset;
						el.classList.remove('iyp_img_deferred');
					}
				}
			}
			 window.addEventListener("scroll", throttle(imgDeferredScroll, 300));
		} else {
	    var observer = new IntersectionObserver(onIntersection, {rootMargin: '200px'});
      for(let element of targetElements) {
        observer.observe(element);
      }
    }
	</script>
